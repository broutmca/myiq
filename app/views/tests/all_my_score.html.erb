<div class="container">
  <h2>All My Score</h2>
  <table class="table table-striped table-bordered">
    <thead class="thead-dark">
      <tr>
        <th>Id#</th>
        <th>view details</th>
        <th>Created At</th>
        <th>Updated at</th>
      </tr>
    </thead>
    <tbody> 
      <% @all_score.each do |score|  %>   
      <tr class="">
        <td><%= score.id %></td>
        <td><%= link_to "score details", test_my_score_path(score) , class: "btn btn-success"%></td>
        <td><%= score.created_at %></td>
        <td><%= score.updated_at %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>